<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
        <ListPreference
            android:key="@string/pref_text_color_key"
            android:title="@string/pref_text_color_title"
            android:dialogTitle="@string/pref_text_color_title"
            android:entries="@array/textColors"
            android:entryValues="@array/textColorsData"
            android:defaultValue="@string/pref_text_color_entryValue_white"
            android:summary="@string/pref_text_color_entry_white"/>

        <CheckBoxPreference
            android:key="@string/pref_key_start_on_boot"
            android:title="@string/pref_title_start_on_boot"
            android:defaultValue="false"/>
        />
</PreferenceScreen>

